programa {
  funcao inicio() 
    {
        real metros, decimetros, centimetros, milimetros

        escreva("Digite um valor em metros: ")
        leia(metros)

        decimetros = metros * 10
        centimetros = metros * 100
        milimetros = metros * 1000

        escreva("\n", metros, " metros equivalem a:\n")
        escreva(decimetros, " decimetros\n")
        escreva(centimetros, " centimetros\n")
        escreva(milimetros, " milimetros\n")
    }
}
  }
}


 2-)

programa {
  funcao inicio() {
      inteiro numero, multiplicador, resultado
      
      para (numero = 1; numero <= 9; numero++){
        escreva("Tabuada do ", numero, ":\n")
        para (multiplicador = 1; multiplicador <= 10; multiplicador++) 
         {
        resultado = numero * multiplicador
        escreva(numero, " x ", multiplicador, " = ", resultado, "\n")
        escreva("\n")
         }
      }
  }
}


  }
}


