programa {
  funcao inicio() 
    {
        inteiro numeros[3]
        inteiro i = 0 

        enquanto (i < 3) {
            escreva("Digite um nÃºmero positivo: ")
            leia(numeros[i])

            se (numeros[i] < 0) {
                escreva("NÃºmero negativo digitado. LaÃ§o encerrado.\n")
                pare 
            }

            i = i + 1 
        }

        escreva("\nNumeros digitados:\n")
        para (inteiro j = 0; j < i; j++) {
            escreva(numeros[j], "\n")
        }
    }
}
  }
}


