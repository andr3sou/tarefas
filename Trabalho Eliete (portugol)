programa {
  funcao inicio() 
    {
        inteiro numeroMaximo, i, j
      
        escreva("Digite um numero maximo impar: ")
        leia(numeroMaximo)
        se (numeroMaximo % 2 == 0) {
        escreva("O numero digitado nao é ímpar. Por favor, digite um nÃºmero Ã­mpar.")
        } senao {
        para (i = 1; i <= numeroMaximo; i++) {
        para (j = i; j <= numeroMaximo; j++) {
        escreva(j, " ")
                }
        escreva("\n")
            }
        }
    }
}
  }
}



