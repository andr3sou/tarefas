programa {
  funcao inicio() {
     inteiro numeros[4], impares[4], pares[4]
     inteiro i, cont_impares = 0, cont_pares = 0, soma_impares = 0, maior_par = -99999, menor_numero = 99999, maior_numero = -99999
    real media_impares
      para (i = 0; i < 4; i++)
        {
      escreva("Digite o ", i + 1, "Âº nÃºmero inteiro: ")
      leia(numeros[i])
      se (numeros[i] % 2 != 0)
            {
      impares[cont_impares] = numeros[i]
      cont_impares++
      soma_impares += numeros[i]
            }
senao
            {
         pares[cont_pares] = numeros[i]
        cont_pares++
           se (numeros[i] > maior_par)
                {
           maior_par = numeros[i]
                }
            }
            se (numeros[i] < menor_numero)
            {
                menor_numero = numeros[i]
            }
            se (numeros[i] > maior_numero)
            {
                maior_numero = numeros[i]
            }
        }
        se (cont_impares > 0)
        {
            media_impares = soma_impares * 1.0 / cont_impares
            escreva("MÃ©dia dos Ã­mpares: ", media_impares, "\n")
        }
        senao
        {
            escreva("NÃ£o hÃ¡ nÃºmeros Ã­mpares.\n")
        }
        se (cont_pares > 0)
        {
            escreva("Maior nÃºmero par: ", maior_par, "\n")
        }
        senao
        {
            escreva("NÃ£o hÃ¡ nÃºmeros pares.\n")
        }
        escreva("DiferenÃ§a entre o maior e o menor nÃºmero: ", maior_numero - menor_numero, "\n")
    }
}

  }
}


 15-)


programa {
  funcao inicio() 
    {
        cadeia nome1, nome2, nome3
        inteiro idade1, idade2, idade3
        inteiro maiorIdade, menorIdade
        cadeia nomeMaisNovo
        real mediaIdades

        escreva("Digite o nome da primeira pessoa: ")
        leia(nome1)
        escreva("Digite a idade da primeira pessoa: ")
        leia(idade1)

        escreva("Digite o nome da segunda pessoa: ")
        leia(nome2)
        escreva("Digite a idade da segunda pessoa: ")
        leia(idade2)

        escreva("Digite o nome da terceira pessoa: ")
        leia(nome3)
        escreva("Digite a idade da terceira pessoa: ")
        leia(idade3)

        maiorIdade = idade1
        menorIdade = idade1
        nomeMaisNovo = nome1

        se (idade2 > maiorIdade) {
            maiorIdade = idade2
        } senao se (idade2 < menorIdade) {
            menorIdade = idade2
            nomeMaisNovo = nome2
        }

        se (idade3 > maiorIdade) {
            maiorIdade = idade3
        } senao se (idade3 < menorIdade) {
            menorIdade = idade3
            nomeMaisNovo = nome3
        }

        mediaIdades = (idade1 + idade2 + idade3) / 3.0

        escreva("\nMaior idade: ", maiorIdade)
        escreva("\nNome da pessoa mais nova: ", nomeMaisNovo)
        escreva("\nMÃ©dia das idades: ", mediaIdades)
    }
}
      
  }
}




