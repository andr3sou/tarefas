programa {
  funcao inicio()

    {
        real fahrenheit, celsius

        escreva("Digite a temperatura em graus Fahrenheit: ")
        leia(fahrenheit)

        celsius = (fahrenheit - 32.0) * (5.0 / 9.0)

        escreva("A temperatura em graus Celsius Ã©: ", celsius)
    }
}
  }
}



 5-)

programa {
      inclua biblioteca Matematica --> mat
  funcao inicio() {
    
        real numero1, numero2, soma, produto, quadrado1, raizQuadrada, senoDiferenca, modulo1

        escreva("Digite o primeiro numero: ")
        leia(numero1)

        escreva("Digite o segundo numero: ")
        leia(numero2)

        soma = numero1 + numero2
        produto = numero1 * mat.potencia(numero2, 2)
        quadrado1 = mat.potencia(numero1, 2)
        raizQuadrada = mat.raiz(mat.potencia(numero1, 2) + mat.potencia(numero2, 2), 2)
        senoDiferenca = mat.seno(numero1 - numero2)

                se (numero1 < 0) {
            modulo1 = -numero1
        } senao {
            modulo1 = numero1
        }

        escreva("\nResultados:\n")
        escreva("a) Soma: ", soma, "\n")
        escreva("b) Produto do primeiro numero pelo quadrado do segundo: ", produto, "\n")
        escreva("c) Quadrado do primeiro numero: ", quadrado1, "\n")
        escreva("d) Raiz quadrada da soma dos quadrados: ", raizQuadrada, "\n")
        escreva("e) Seno da diferenÃ§a do primeiro numero pelo segundo: ", senoDiferenca, "\n")
        escreva("f) MÃ³dulo do primeiro numero: ", modulo1, "\n")
    }
}
  }
}


 6-)


programa {
  funcao inicio() 
    {
        inteiro valor1, valor2, i

        escreva("Digite o primeiro valor: ")
        leia(valor1)

        escreva("Digite o segundo valor: ")
        leia(valor2)

        se (valor1 < valor2) {
            para (i = valor1; i <= valor2; i++) {
                escreva(i, " ")
            }
        } senao se (valor1 > valor2) {
            para (i = valor1; i >= valor2; i--) {
                escreva(i, " ")
            }
        } senao {
            escreva("Valores iguais")
        }
    }
}
  }
}



 7-)

programa {
  funcao inicio() {
  cadeia tipoProduto
        inteiro quantidade
        real precoUnitario, valorTotal, desconto

        escreva("Qual o tipo de produto? (tecnologia/vestuario): ")
        leia(tipoProduto)

        escreva("Quantas unidades? ")
        leia(quantidade)

        escreva("Qual o preÃ§o de cada unidade? ")
        leia(precoUnitario)

        valorTotal = precoUnitario * quantidade
        desconto = 0.0

        se (tipoProduto == "tecnologia" e quantidade > 2) {
            desconto = valorTotal * 0.10 // 10% de desconto
        } senao se (tipoProduto == "vestuario" e quantidade > 3) {
            desconto = valorTotal * 0.20 // 20% de desconto
        }

        valorTotal = valorTotal - desconto

        escreva("Valor total da compra: R$ ", valorTotal)
    }
}
    
  }
}


 8-)
 
programa {
  funcao inicio() {
    
        real temperatura

        escreva("Digite a temperatura atual em graus Celsius: ")
        leia(temperatura)

        se (temperatura > 30.0) {
            escreva("Esta quente!")
        } senao se (temperatura >= 20.0 e temperatura <= 30.0) {
            escreva("A temperatura esta agradavel.")
        } senao {
            escreva("Esta frio.")
        }
    }
} 
  }
}


9-)

programa {
  funcao inicio() 
    {
        real valorPedido, taxaEntrega
        logico membroFidelidade
        cadeia resposta

        escreva("Qual o valor do pedido? R$ ")
        leia(valorPedido)

        escreva("Voce e membro do programa de fidelidade? (sim/nÃ£o) ")
        leia(resposta)

        se (resposta == "sim") {
            membroFidelidade = verdadeiro
        } senao {
            membroFidelidade = falso
        }

        se (valorPedido < 10.0) {
            taxaEntrega = 5.0
        } senao se (valorPedido >= 10.0 e valorPedido < 20.0) {
            taxaEntrega = 3.0
        } senao {
            taxaEntrega = 0.0
        }

        se (membroFidelidade e taxaEntrega > 0.0) {
            taxaEntrega = taxaEntrega - 2.0
        }

        escreva("A taxa de entrega Ã© de R$ ", taxaEntrega)
    }
}
  }
}


